<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Fallback>
            <Action ID="Search"/>
            <Sequence>
                <Action ID="NavObjective"/>
                <ReactiveFallback>
                    <Action ID="StoreObject"/>
                </ReactiveFallback>
                <Action ID="NavOdom"/>
            </Sequence>
            <Action ID="NavOdom"/>
        </Fallback>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="NavObjective"/>
            <input_port name="waypoint"/>
        <Action ID="NavOdom"/>
        <Action ID="Search"/>
            <output_port name="goal"/>
        <Action ID="StoreObject"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
